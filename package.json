{
  "name": "web-cell-cli",
  "version": "0.8.1",
  "description": "Developer command-line tool for WebCell",
  "keywords": [
    "developer",
    "command-line",
    "web-cell",
    "component",
    "package",
    "bundle"
  ],
  "author": "shiy2008@gmail.com",
  "license": "AGPL-3.0",
  "homepage": "https://web-cell.tk/DevCLI/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/DevCLI.git"
  },
  "bugs": {
    "url": "https://github.com/EasyWebApp/DevCLI/issues"
  },
  "main": "dist/Component.js",
  "bin": {
    "web-cell": "dist/index.js"
  },
  "directories": {
    "lib": "source",
    "test": "test"
  },
  "engines": {
    "node": "^6.3.0"
  },
  "scripts": {
    "test": "mocha --exit",
    "debug": "mocha --inspect-brk",
    "lint": "eslint source/ test/ --fix",
    "pack": "babel source/ -d dist/ -s",
    "prepublishOnly": "npm test  &&  npm run lint  &&  npm run pack",
    "build": "npm run prepublishOnly  &&  esdoc",
    "start": "web-server docs/ -o",
    "install": "node dist/install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build  &&  git add ."
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.1",
    "@tech_query/node-toolkit": "^0.9.0",
    "amd-bundle": "^1.7.4",
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "jsdom": "^13.2.0",
    "less": "^3.9.0",
    "node-fetch": "^2.3.0",
    "puppeteer-browser": "^0.3.7",
    "sass": "^1.16.1",
    "stylus": "^0.54.5",
    "web-cell": "^0.10.12"
  },
  "peerDependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "web-cell": "^0.10.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-external-webapi-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.13.0",
    "husky": "^1.3.1",
    "koapache": "^1.0.4",
    "mocha": "^5.2.0",
    "should": "^13.2.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.3.0"
          }
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": true
        }
      ],
      "@babel/plugin-transform-runtime"
    ]
  }
}
