{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "6.3.0"
                }
            }
        ]
    ],
    "plugins": [
        [
            "babel-plugin-inline-import",
            {
                "extensions": [
                    ".json"
                ]
            }
        ],
        [
            "@babel/plugin-proposal-decorators",
            {
                "decoratorsBeforeExport": true
            }
        ],
        "@babel/plugin-transform-runtime"
    ]
}
